# Zenzap API Configuration
# Copy this file to .env and fill in your credentials

# API Base URL
API_BASE_URL=https://api.zenzap.co

# Bot Credentials
# Get these from your Zenzap dashboard
BOT_API_KEY=your_bot_api_key_here
BOT_SECRET=your_bot_secret_here

# Member IDs for testing (comma-separated)
# These are UUIDs of members in your organization
MEMBER_IDS=member-id-1,member-id-2
